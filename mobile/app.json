{
  "expo": {
    "name": "Kinevo",
    "slug": "kinevo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "kinevo",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinevo.mobile",
      "appleTeamId": "3D5M22J52M",
      "entitlements": {
        "com.apple.developer.healthkit": true
      },
      "infoPlist": {
        "NSSupportsLiveActivities": true,
        "NSHealthShareUsageDescription": "O Kinevo usa dados de saúde para mostrar frequência cardíaca durante o treino.",
        "NSHealthUpdateUsageDescription": "O Kinevo salva dados do treino no Apple Saúde."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#18181b"
      },
      "package": "com.kinevo.mobile",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "O Kinevo precisa de acesso à sua galeria para você alterar sua foto de perfil."
        }
      ],
      "@bacons/apple-targets",
      "./plugins/with-watch-app"
    ]
  }
}
